Host hocuspokus.vm.bytemark.co.uk
  User admin
  Compression yes
  ForwardX11 no
  ForwardAgent true

  DynamicForward 7777

Host ?.access.*.cloud.bbc.co.uk
  IdentityFile ~/.ssh/id_rsa

Host *,??-*-?
  User darren_oakley
  IdentityFile ~/.ssh/id_rsa
  ProxyCommand >&1; h="%h"; r=${h##*,}; i=${h%%,*}; v=$(($(cut -d. -f2 <<<$i) / 32)); exec ssh -q -p 22000 bastion-tunnel@$v.access.$r.cloud.bbc.co.uk nc $i %p
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host heroku.hubot
  HostName heroku.com
  IdentityFile "/Users/d.oakley/.ssh/identity.heroku.hubot"
  IdentitiesOnly yes

Host heroku.bandiera
  HostName heroku.com
  IdentityFile "/Users/d.oakley/.ssh/identity.heroku.bandiera"
  IdentitiesOnly yes
